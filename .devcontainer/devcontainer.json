{
  "name": "busybox",
  "dockerComposeFile": [
    "docker-compose.yaml"
  ],
  "service": "busybox",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers"
      ]
    }
  },
  "shutdownAction": "stopCompose",
  "features": {
    "git": "os-provided",
    "sshd": "latest"
  }
}
